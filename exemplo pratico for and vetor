package br.unipar;

import java.util.Scanner;

public class ExemploPraticoRepeticao {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int[] vetor = new int[5] ;

        for (int i = 0; i < 5; i++) {
            System.out.println("informe o seu salÃ¡rio: ");
            vetor[i] = scanner.nextInt();
        }
    }
}
